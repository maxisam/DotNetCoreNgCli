{
  "name": "client-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start:s": "dotnet watch run",
    "start:s:p": "dotnet watch run --launch-profile dotNetCoreNgCli-prod",
    "start": "concurrently \"npm run start:s\" \"npm run start:ng\"  ",
    "start:p": "npm run build:ng:p:ssr && npm run start:s:p",
    "start:ng": "ng serve --open -sm -vc -hmr",
    "build:ng": "ng build",
    "build:ng:p": "npm run lint && ng build --prod --buildOptimizer --statsJson --output-hashing=none --extractCss --vendor-chunk",
    "build:ng:p:ssr": "npm run build:ng:p && ng build --prod --app 1 --output-hashing=none",
    "build:ng:p:analyze": "npm run build:ng:p:ssr && npm run report",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.4.0-RC.0",
    "@angular/common": "^4.4.0-RC.0",
    "@angular/compiler": "^4.4.0-RC.0",
    "@angular/core": "^4.4.0-RC.0",
    "@angular/forms": "^4.4.0-RC.0",
    "@angular/http": "^4.4.0-RC.0",
    "@angular/platform-browser": "^4.4.0-RC.0",
    "@angular/platform-browser-dynamic": "^4.4.0-RC.0",
    "@angular/platform-server": "^4.4.0-RC.0",
    "@angular/router": "^4.4.0-RC.0",
    "@nguniversal/aspnetcore-engine": "^1.0.0-beta.3",
    "aspnet-prerendering": "^3.0.1",
    "core-js": "^2.4.1",
    "rxjs": "^5.4.3",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.17"
  },
  "devDependencies": {
    "@angular/cli": "1.4.1",
    "@angular/compiler-cli": "^4.4.0-RC.0",
    "@angular/language-service": "^4.4.0-RC.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.1.2",
    "concurrently": "^3.5.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "~2.5.2",
    "webpack-bundle-analyzer": "^2.9.0"
  }
}
